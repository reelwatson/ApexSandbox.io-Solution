public List<sObject> getListOfsObject(List<Account> accounts, List<Contact> contacts)
{
    // code here
    List<sObject> allSObjects = new List<sObject>();
    if (accounts != null) {
        allSObjects.addAll(accounts);
    }
    if (contacts != null) {
        allSObjects.addAll(contacts);
    }

    return allSObjects;
}