public List<Integer> fibonacciSeries(Integer n) {
    //code here
    // TO DO: refactor for efficiency? Feel like could be more efficient

    Integer integer1 = 1;
    Integer integer2 = 1;
    Integer fibonacciValue;

    List<Integer> fibonacciList = new List<Integer> {1};

    if (n == 2) {
        fibonacciList.add(1);
    }

    else if (n >= 3) {
        fibonacciList.add(1);
        for (Integer i = 3; i <= n; i++) {
            fibonacciValue = integer1 + integer2;
            fibonacciList.add(fibonacciValue);
            integer1 = integer2;
            integer2 = fibonacciValue;
        }
    }

return fibonacciList;
    
}