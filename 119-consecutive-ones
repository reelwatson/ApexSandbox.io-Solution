public Integer maxConsecutiveOnes(Integer[] numbers) {
    //code here

    //Loop in a loop? To track current sum of consecutive ones vs largest sum so far in array
    Integer consecutiveOnesSoFar = 0;
    Integer longestChainOfOnes = 0;

    for (Integer num : numbers) {
        if (num == 1) {
            consecutiveOnesSoFar++;
        }
        else {
            if (consecutiveOnesSoFar > longestChainOfOnes) {
                longestChainOfOnes = consecutiveOnesSoFar;
            }
            consecutiveOnesSoFar = 0;
        }
    }
    return consecutiveOnesSoFar;
}