public List<String> fullNames(List<String> firstNames, List<String> lastNames) {
    //code here
    List<String> fullNameList = new List<String>();
    String fullName = '';

    // Loop through list and check for first names
    for (Integer i = 0; i < firstNames.size(); i++) {
        if (firstNames[i] != null && firstNames[i] != '') {
            // Add first name to fullName string if evaluates to true
            fullName = firstNames[i];
        }

        // Check if last name entry is not null or blank
        if (lastNames[i] != null && lastNames[i] != '') {
            // Add last name to fullName string if evaluates to true
            fullName = fullName + ' ' + lastNames[i];
        }

        // Add fullName string to fullNameList
        fullNameList.add(fullName);
    }
    return fullNameList;
}