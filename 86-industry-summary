public Map<String, Decimal> industrySummary(List<Account> accounts) {
    //code here
    Map<String, Decimal> industryMap = new Map<String, Decimal>();

    for (Account acc : accounts) {
        if (industryMap.containsKey(acc.Industry)) {
            // industryMap.get(acc.Industry).add(acc.AnnualRevenue);
            Decimal priorAnnualRevenue = industryMap.get(acc.Industry);
            industryMap.put(acc.Industry, priorAnnualRevenue + acc.AnnualRevenue);
        }
        else {
            industryMap.put(acc.Industry, acc.AnnualRevenue);
        }
    }

    return industryMap;
}