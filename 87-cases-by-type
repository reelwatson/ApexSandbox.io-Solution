public Map<String, List<Case>> casesByType(List<Case> cases) {
    //code here
    Map<String, List<Case>> casesMap = new Map<String,List<Case>>();
    for (Case c : cases) {
        if (casesMap.containsKey(c.Type)) {
            // add 
            casesMap.get(c.Type).add(c);
        }
        else {
            casesMap.put(c.Type, new List{c});
        }
    }

    return casesMap;
}