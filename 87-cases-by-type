public Map<String, List<Case>> casesByType(List<Case> cases) {
    //code here
    Map<String, List<Case>> casesMap = new Map<String,List,<Case>>();
    for (Case case : cases) {
        if (casesMap.containsKey(case.Type)) {
            // add 
            casesMap.get(case.Type).add(case);
        }
        else {
            casesMap.put(case.Type, new List{case});
        }
    }

    return casesMap;
}